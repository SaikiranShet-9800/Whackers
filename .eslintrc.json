{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    // Add custom ESLint rules here
    "no-unused-vars": "warn",
    "no-console": "off" 
  },
  "ignorePatterns": ["node_modules/", "dist/", "google-credentials.json"]
}